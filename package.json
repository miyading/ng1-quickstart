    "buildUiRouter": "browserify uiRouter/script.js > uiRouter/bundle.js",
    "solarSystem": "browserify uiRouter/helloSolarSystem/script.js uiRouter/helloSolarSystem/*/*.js uiRouter/helloSolarSystem/*/*.json > uiRouter/helloSolarSystem/bundle.js"
