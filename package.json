    "buildScope": "browserify scopes/script.js > scopes/bundle.js",
    "buildExpression": "browserify expression/script.js > expression/bundle.js",
    "buildFilterArray": "browserify filters/filterArray/script.js > filters/filterArray/bundle.js",
    "buildFilterArray2": "browserify filters/filterArray2/script.js > filters/filterArray2/bundle.js",
    "buildStatefulFilter": "browserify filters/statefulFilter/script.js > filters/statefulFilter/bundle.js",
