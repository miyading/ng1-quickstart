    "buildInvoice": "browserify invoiceApp/invoice1.js > invoiceApp/bundle.js",
    "buildInvoice2": "browserify invoiceApp2/finance2.js invoiceApp2/invoice2.js > invoiceApp2/bundle.js",
    "buildInvoice3": "browserify invoiceApp3/finance3.js invoiceApp3/invoice3.js > invoiceApp3/bundle.js",
